{
    "grabberfile":{
        "required":{
            "active_sources":{
				"types":["list",
					"sourceid"]},
            "cattrans":{
				"types":["dict",
					"list",
					"list",
					"string"]},
            "cattrans_unknown":{
				"types":["string"]},
            "channel_groups":{
				"types":["numbered dict",
					"string"]},
            "data_version":{
				"types":["integer"]},
            "language_texts":{
				"types":["dict",
					"string"]},
            "program_version":{
				"types":[{"type":"list", "length":3}, 
					"integer"]},
            "roletrans":{
				"types":["dict",
					"list",
					"string"]},
            "sources":{
				"types":["numbered dict",
					{"type":"dict", 
						"required":[{"name":"json file", "type":"string"},
							{"name":"version", "type":"integer","default": 0}],
						"sugested":[{"name":"cattrans type", "in":[null,1,2], "default": null},
							{"name":"cattransid", "type":["integer","none"], "default": null}],
						"optional":[{"name":"json_url", "type":"url", "default": "https://raw.githubusercontent.com/tvgrabbers/sourcematching/master/sources"}]}]},
            "source_channels":{
				"types":[{"type":"numbered dict", "keys": "sourceid"},
					"dict",
					"string"]},
            "unknown_program_title":{
				"types":["string"]}},
        "sugested":{
            "channel_grouping":{
				"types":[{"type":"numbered dict", "keys": "groupid"},
					"list", 
					"chanid"]},
            "fetch-timezone":{
				"types":["tz-string"], 
				"default":"UTC"},
            "groupslot_names":{
				"types":["list", 
					"string"]},
            "logo_names":{
				"types":[{"type":"dict","keys":"chanid"},
					{"type":"list", "length":2, "items":["logoid", "string"]}]},
            "movie_genres":{
				"types":["list", "string"]},
            "prime_source":{
				"types":[{"type":"dict","keys":"chanid"}, 
					"sourceid"]},
            "prime_source_groups":{
				"types":[{"type":"numbered dict", "keys": "sourceid"},
					"groupid"]},
            "prime_source_order":{
				"types":["list", 
					"sourceid"]},
            "rating":{
				"types":[
					{"type":"dict", 
						"required":[{"name":"name", "type":"string"},
							{"name":"unique_codes","type":"dict"},
							{"name":"addon_codes","type":"dict"}]}, 
					{"type":["string", "dict"]}, 
					{"type":"dict", 
						"required":[{"name":"code", "type":"string"},
						{"name":"text", "type":"string"},
						{"name":"icon", "type":"string"}]}]},
            "series_genres":{
				"types":["list", "string"]},
            "sourceid_order":{
				"types":["list",
					"sourceid"]},
            "sports_genres":{
				"types":["list", "string"]},
            "version_message":{
				"types":["list",
					"string"]},
            "warning_message":{
				"types":["numbered dict",
					"list", 
					"string"]}},
        "optional":{
            "channel_rename":{
				"types":[{"type":"dict","keys":"chanid"},
					"string"]},
            "channelprogram_rename":{
				"types":[{"type":"dict","keys":"chanid"}, 
					"dict",
					"string"]},
            "combined-channels-tz":{
				"types":["tz-string"], 
				"default":"UTC"},
            "combined_channels":{
				"types":[{"type":"dict","keys":"chanid"}, 
					{"type":"list"},
					{"type":["chanid","dict"],
						"required":[{"name":"chanid", "type":"chanid"}],
						"sugested":[{"name":"start", "type":"time","default": "0:00 (see the WIKI)"},
							{"name":"end", "type":"time","default": "23:59(see the WIKI)"}],
						"optional":[{"name":"slots", "type":"list"}]}]},
            "compat_text":{
				"types":["string"]},
            "description":{
				"types":["list", "string"]},
            "detail_sources":{
				"types":["list", 
					"sourceid"]},
            "detailed_genres":{
				"types":["list",
					"string"]},
            "episode exclude genres":{
				"types":["list",
					"string"]},
            "generic_channel_genres":{
				"types":["dict",
					"list",
					"chanid"]},
            "group language":{
				"types":[{"type":"numbered dict", "keys": "groupid"}, 
					{"type":"string", "length":2}], 
				"default":"en"},
            "group_order":{
				"types":["list", 
					"groupid"]},
            "groupnameremove":{
				"types":["list",
					"string"]},
            "language":{
				"types":[{"type":"string", "length":2}], 
				"default":"en"},
            "logo_provider":{
				"types":["numbered dict",
					"string"]},
			"logo_source_preference":{
				"types":["list",
					"sourceid"]},
            "merge_into":{
				"types":[{"type":"dict","keys":"chanid"}, 
					{"type":"dict", 
						"required":[{"name":"chanid", "type":"chanid"},
							{"name":"sources", "type":"numbered dict", "keys": "sourceid","types": "string"}],
						"sugested":[{"name":"date", "type":"date"},
							{"name":"message", "type":["string","list"]}]}]},
            "source-url":{
				"types":["url"], 
				"default":"https://raw.githubusercontent.com/tvgrabbers/sourcematching/master/sources"},
            "titlerename":{
				"types":["dict"]},
            "ttvdb disable groups":{
				"types":["list", 
					"groupid"]},
            "ttvdb langs":{
				"types":["list", 
					{"type":"string", "length":2}], 
				"default":["en"]}}},
    "sourcefile":{
        "required":{
            "file-name":{
				"types":["string"]},
            "name":{
				"types":["string"]},
            "version":{
				"types":["integer"]},
            "api-version":{
				"types":[{"type":"list", "length":3}, 
					"integer"]}},
        "sugested":{
            "language":{
				"types":[{"type":"string", "length":2}], 
				"default":"en"},
            "site-timezone":{
				"types":["tz-string"], 
				"default":"UTC"},
            "empty_channels":{
				"types":["list", "string"]},
            "rating":{
				"types":[{"type":"dict"},
					{"type":"string"}]},
            "cattrans":{
				"types":[{"type":"dict"},
					{"type":["string","dict"],
						"required":[{"name":"default", "type":"list"}],
						"allowed":"all"},
					{"type":"list", "length":2},
					{"type":"string"}
					]}},
        "optional":{
            "description":{
				"types":["list", "string"]},
            "detail_processor":{
				"types":["boolean"],
				"default":false},
            "without-full-timings":{
				"types":["boolean"],
				"default":false},
            "night-date-switch":{
				"types":["integer"],
				"default":0},
            "no_genric_matching":{
				"types":["list", "string"]},
            "alt-channels":{
				"types":[{"type":"dict"},
					{"type":"list", "length":2}]},
            "cattrans_keywords":{
				"types":[{"type":"dict"},
					{"type":"string"}]},
			"empty-values":{
				"types":[{"type":"list"},
					{"type":["string","none"]}]}},
		"ignore_keys":["is_virtual","login","error","seriesid","seriesname","episodes","episodecount","episode","lang-list"]},
    "virtualsource":{
        "required":{
            "file-name":{
				"types":["string"]},
            "name":{
				"types":["string"]},
            "version":{
				"types":["integer"]},
            "api-version":{
				"types":[{"type":"list", "length":3}, 
					"integer"]}},
        "sugested":{
            "language":{
				"types":[{"type":"string", "length":2}], 
				"default":"en"}},
        "optional":{
            "description":{
				"types":["list", "string"]}},
        "ignore_keys":["is_virtual"],
        "unused_keys":[
			"detail_processor","site-timezone","empty_channels","rating","cattrans","without-full-timings",
			"night-date-switch","no_genric_matching","alt-channels","cattrans_keywords","empty-values"]},
	"data_defs":["channels","base-channels","base","detail","detail2"],
	"channel_keys":{
        "required":{
			"name":{
				"types":["string"]}},
        "optional":{
			"group":{
				"types":["groupid"]},
			"icon":{
				"types":["string"]},
			"icongrp":{
				"types":["logoid"]},
			"fetch_grp":{
				"types":["string"]},
			"HD":{
				"types":["boolean"]}}},
	"data_def_keys":{
		"channels":{
			"required":{},
			"optional":{
				"url_type":{
					"types":["integer"],
					"default":0}},
			"value_keys":{
				"required":["channelid","name","fetch_grp"],
				"optional":["group","icon","HD"]}},
		"base-channels":{
			"required":{},
			"optional":{
				"url_type":{
					"types":["integer"],
					"default":0}},
			"value_keys":{
				"required":["channelid","name","fetch_grp"],
				"optional":["group","icon","HD"]}},
		"base":{
			"required":{
				"url_type":{
					"types":["integer"],
					"default":0}},
			"optional":{
				"max days":{
					"types":["integer"],
					"default":14},
				"default-item-count":{
					"required_on":[12,13,14],
					"types":["integer"],
					"default":0}},
			"data_keys":{
				"optional":{
					"today":{
						"types":["value_def"]},
					"date-check":{
						"types":["value_def"]},
					"page-item-count":{
						"required_on":[12,13,14],
						"types":["value_def"]},
					"total-item-count":{
						"required_on":[12,13,14],
						"types":["value_def"]}}},
			"value_keys":{
				"required":["start-time","stop-time","name","channelid"],
				"detail":["prog_ID","detail_url"],
				"optional":["length","alt-start-time","alt-stop-time","gen_ID",
					"episode title","originaltitle","description","genre","subgenre","genres",
					"premiere year","infourl","audio","star-rating","broadcaster","airdate",
					"rerun","premiere","last-chance","new","teletext","season","episode",
					"episodecount","country","rating","HD","widescreen","blackwhite",
					"credits","director","actor","guest","writer","composer","presenter",
					"reporter","commentator","adapter","producer","editor"]}},
		"detail":{
			"required":{
				"provides":{
					"types":["list","integer"]}},
			"optional":{
				"url_type":{
					"types":["integer"],
					"default":0}},
			"value_keys":{
				"required":[],
				"optional":["start-time","stop-time","name","channelid",
					"prog_ID","detail_url","length","alt-start-time","alt-stop-time","gen_ID",
					"episode title","originaltitle","description","genre","subgenre","genres",
					"premiere year","infourl","audio","star-rating","broadcaster","airdate",
					"rerun","premiere","last-chance","new","teletext","season","episode",
					"episodecount","country","rating","HD","widescreen","blackwhite",
					"credits","director","actor","guest","writer","composer","presenter",
					"reporter","commentator","adapter","producer","editor"]}},
		"detail2":{
			"required":{
				"provides":{
					"types":["list","integer"]}},
			"optional":{
				"url_type":{
					"types":["integer"],
					"default":0}},
			"value_keys":{
				"required":[],
				"optional":["start-time","stop-time","name","channelid",
					"prog_ID","detail_url","length","alt-start-time","alt-stop-time","gen_ID",
					"episode title","originaltitle","description","genre","subgenre","genres",
					"premiere year","infourl","audio","star-rating","broadcaster","airdate",
					"rerun","premiere","last-chance","new","teletext","season","episode",
					"episodecount","country","rating","HD","widescreen","blackwhite",
					"credits","director","actor","guest","writer","composer","presenter",
					"reporter","commentator","adapter","producer","editor"]}}}
		}
