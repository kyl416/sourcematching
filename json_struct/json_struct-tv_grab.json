{"version":0,
    "select_struct_list":[
		{"testdata":"struct-grabberfile",
			"description":"The file %s seems to be a grabber-JSON file\\\n",
			"title":"tv_grab_[a-z][a-z]\\.json",
			"type":"dict"},
		{"testdata":"struct-virtualsource",
			"description":"The file %s seems to be a virtual-source-JSON file\\\n",
			"title":"source-.*?\\.json",
			"type":"dict", 
			"keyvalues":[
				{"key":"is_virtual", "value":true, "default":false}]},
		{"testdata":"struct-sourcefile",
			"description":"The file %s seems to be a source-JSON file\\\n",
			"title":"source-.*?\\.json",
			"type":"dict", 
			"keyvalues":[
				{"key":"is_virtual", "value":false, "default":false},
				{"key":"detail_processor", "value":false, "default":false}]},
		{"testdata":"struct-detailedsource",
			"description":"The file %s seems to be a detailed-source-JSON file\\\n",
			"title":"source-.*?\\.json",
			"type":"dict", 
			"keyvalues":[
				{"key":"is_virtual", "value":false, "default":false},
				{"key":"detail_processor", "value":true, "default":false}]}],
	"struct-grabberfile":{
		"type":"dict",
        "lookup_lists":{
			"lst-sourceid":{"keyword": "sources","values": "intkeys"},
			"lst-chanid":{"keyword": "source_channels","values": "keys","level": 2},
			"lst-groupid":{"keyword": "channel_groups","values": "intkeys"},
			"lst-logoid":{"keyword": "logo_provider","values": "intkeys"},
			"lst-cattranstype":{"list":[null,1,2]}},
		"required":{
            "active_sources":{
				"types":[
					"list",
					"lst-sourceid"]},
            "cattrans":{
				"types":[
					"dict",
					"list",
					"list",
					"string"]},
            "cattrans_unknown":{
				"types":["string"]},
            "channel_groups":{
				"types":[
					"numbered dict",
					"string"]},
            "data_version":{
				"types":["integer"]},
            "language_texts":{
				"types":[
					"dict",
					"string"]},
            "program_version":{
				"types":[
					{"type":"list", "length":3}, 
					"integer"]},
            "roletrans":{
				"types":["dict",
					"list",
					"string"]},
            "sources":{
				"types":[
					"numbered dict",
					{"type":"dict", 
						"required":{
							"json file":{
								"types":["string"]},
							"version":{
								"types":["integer"],
								"default": 0}},
						"sugested":{
							"cattrans type":{
								"types":["lst-cattranstype"], 
								"default": null},
							"cattransid":{
								"types":[["integer","none"]], 
								"default": null}},
						"optional":{
							"json_url":{
								"types":["url"], 
								"default": "https://raw.githubusercontent.com/tvgrabbers/sourcematching/master/sources"}}}]},
            "source_channels":{
				"types":[
					{"type":"numbered dict", "keys": "lst-sourceid"},
					"dict",
					"string"]},
            "unknown_program_title":{
				"types":["string"]}},
        "sugested":{
            "channel_grouping":{
				"types":[
					{"type":"numbered dict", "keys": "lst-groupid"},
					"list", 
					"lst-chanid"]},
            "fetch-timezone":{
				"types":["tz-string"], 
				"default":"UTC"},
            "groupslot_names":{
				"types":[
					"list", 
					"string"]},
            "logo_names":{
				"types":[
					{"type":"dict","keys":"lst-chanid"},
					{"type":"list", "length":2, "items":["lst-logoid", "string"]}]},
            "movie_genres":{
				"types":[
					"list", 
					"string"]},
            "prime_source":{
				"types":[
					{"type":"dict","keys":"lst-chanid"}, 
					"lst-sourceid"]},
            "prime_source_groups":{
				"types":[
					{"type":"numbered dict", "keys": "lst-sourceid"},
					"lst-groupid"]},
            "prime_source_order":{
				"types":[
					"list", 
					"lst-sourceid"]},
            "rating":{
				"types":[
					{"type":"dict", 
						"required":{
							"name":{"types":["string"]},
							"unique_codes":{"types":["dict"]},
							"addon_codes":{"types":["dict"]}}}, 
					{"type":["string", "dict"]}, 
					{"type":"dict", 
						"required":{
							"code":{"types":["string"]},
							"text":{"types":["string"]},
							"icon":{"types":["string"]}}}]},
            "series_genres":{
				"types":[
					"list", 
					"string"]},
            "sourceid_order":{
				"types":[
					"list",
					"lst-sourceid"]},
            "sports_genres":{
				"types":[
					"list", 
					"string"]},
            "version_message":{
				"types":[
					["list","string"],
					"string"]},
            "warning_message":{
				"types":[
					"numbered dict",
					["list","string"],
					"string"]}},
        "optional":{
            "channel_rename":{
				"types":[
					{"type":"dict","keys":"lst-chanid"},
					"string"]},
            "channelprogram_rename":{
				"types":[
					{"type":"dict","keys":"lst-chanid"}, 
					"dict",
					"string"]},
            "combined-channels-tz":{
				"types":["tz-string"], 
				"default":"UTC"},
            "combined_channels":{
				"types":[
					{"type":"dict","keys":"lst-chanid"}, 
					{"type":"list"},
					{"type":["lst-chanid","dict"],
						"required":{
							"chanid":{
								"types":["lst-chanid"]}},
						"sugested":{
							"start":{
								"types":["time"],
								"default": "0:00 (see the WIKI)"},
							"end":{
								"types":["time"],
								"default": "23:59(see the WIKI)"}},
						"optional":{
							"slots":{"types":["list"]}}}]},
            "compat_text":{
				"types":["string"]},
            "description":{
				"types":[
					["list","string"],
					"string"]},
            "detail_sources":{
				"types":[
					"list", 
					"lst-sourceid"]},
            "detailed_genres":{
				"types":[
					"list",
					"string"]},
            "episode exclude genres":{
				"types":[
					"list",
					"string"]},
            "generic_channel_genres":{
				"types":[
					"dict",
					"list",
					"lst-chanid"]},
            "group language":{
				"types":[
					{"type":"numbered dict", "keys": "lst-groupid"}, 
					{"type":"string", "length":2}], 
				"default":"en"},
            "group_order":{
				"types":[
					"list", 
					"lst-groupid"]},
            "groupnameremove":{
				"types":[
					"list",
					"string"]},
            "language":{
				"types":[{"type":"string", "length":2}], 
				"default":"en"},
            "logo_provider":{
				"types":[
					"numbered dict",
					"string"]},
			"logo_source_preference":{
				"types":[
					"list",
					"lst-sourceid"]},
            "merge_into":{
				"types":[
					{"type":"dict","keys":"lst-chanid"}, 
					{"type":"dict", 
						"required":{
							"chanid":{
								"types":["lst-chanid"]},
							"sources":{
								"types":[
									{"type":"numbered dict", "keys": "lst-sourceid"},
									"string"]}},
						"sugested":{
							"date":{
								"types":["date"]},
							"message":{
								"types":[
									["list","string"],
									"string"]}}}]},
            "source-url":{
				"types":["url"], 
				"default":"https://raw.githubusercontent.com/tvgrabbers/sourcematching/master/sources"},
            "titlerename":{
				"types":["dict"]},
            "ttvdb disable groups":{
				"types":[
					"list", 
					"lst-groupid"]},
            "ttvdb langs":{
				"types":[
					"list", 
					{"type":"string", "length":2}], 
				"default":["en"]}}},
    "struct-sourcefile":{
		"type":"dict",
        "lookup_lists":{
			"lst-link_funcid":{"list":[101,102,103,104,105,106,201,202],"append":true}},
		"either":[
			{"required":{
				"channels":{
					"types":[
						["struct-channel_list","struct-channels"]]}},
			"optional":{
				"base-channels":{
					"types":["struct-channels"]}}},
			{"required":{
				"base-channels":{
					"types":["struct-channels"]}
						},
			"optional":{
				"channels":{
					"types":[
						["struct-channel_list","struct-channels"]]}}}],
        "required":{
			"base":{
				"types":["struct-base"]},
            "api-version":{
				"types":[
					{"type":"list", "length":3}, 
					"integer"]},
            "file-name":{
				"types":["string"]},
            "name":{
				"types":["string"]},
            "version":{
				"types":["integer"]}},
		"sugested":{
            "language":{
				"types":[{"type":"string", "length":2}], 
				"default":"en"},
            "site-timezone":{
				"types":["tz-string"], 
				"default":"UTC"},
            "empty_channels":{
				"types":[
					"list", 
					"string"]},
            "rating":{
				"types":[
					{"type":"dict"},
					{"type":"string"}]},
            "cattrans":{
				"types":[
					{"type":"dict"},
					{"type":["string","dict"],
						"required":{
							"default":{"types":["list"]}},
						"allowed":"all"},
					{"type":"list", "length":2},
					{"type":"string"}]}},
        "optional":{
            "description":{
				"types":[
					["list","string"],
					"string"]},
            "without-full-timings":{
				"types":["boolean"],
				"default":false},
            "night-date-switch":{
				"types":["integer"],
				"default":0},
            "no_genric_matching":{
				"types":[
					"list",
					"string"]},
            "alt-channels":{
				"types":[
					{"type":"dict"},
					{"type":"list", "length":2}]},
            "cattrans_keywords":{
				"types":[
					{"type":"dict"},
					{"type":"string"}]},
			"empty-values":{
				"types":[
					{"type":"list"},
					{"type":["string","none"]}]}},
        "unused_keys":["detail","detail2"],
		"ignore_keys":["is_virtual","detail_processor",
					"login","error","seriesid","seriesname","episodes","episodecount","episode","lang-list"]},
    "struct-virtualsource":{
		"include":"struct-sourcefile",
		"either":[],
        "required":{
			"channels":{
				"types":["struct-channel_list"]}},
        "unused_keys":[
			"detail_processor","site-timezone","empty_channels","rating","cattrans","without-full-timings",
			"night-date-switch","no_genric_matching","alt-channels","cattrans_keywords","empty-values",
			"base-channels","base"]},
	"struct-detailedsource":{
		"include":"struct-sourcefile",
        "required":{
			"base":{
				"types":["struct-base_detailed"]},
			"detail":{
				"types":["struct-detail"]}},
        "optional":{
			"detail2":{
				"types":["struct-detail"]}}},
	"struct-channel_list":{
		"type":"dict",
		"required":{
			"data":{
				"types":[
					"dict", 
					{"type":"dict",
						"required":{
							"name":{
								"types":["string"]}},
						"optional":{
							"group":{
								"types":["lst-groupid"]},
							"icon":{
								"types":["string"]},
							"icongrp":{
								"types":["lst-logoid"]},
							"fetch_grp":{
								"types":["string"]},
							"HD":{
								"types":["boolean"]}}}]}},
		"forbidden_keys":["values"],
		"ignore_keys":["url"]},
	"struct-channels":{
		"include":"struct-data_def_full_extended",
		"required":{
			"values":{
				"types":[
					{"type":"dict",
						"required":{
							"channelid":{
								"types":["struct-link_def"]},
							"name":{
								"types":["struct-link_def"]}},
						"sugested":{
							"fetch_grp":{
								"types":["struct-link_def"]}},
						"optional":{
							"group":{
								"types":["struct-link_def"]},
							"icon":{
								"types":["struct-link_def"]},
							"HD":{
								"types":["struct-link_def"]}}}]}},
		"optional":{
			"text_replace":{
				"types":[
					{"type":"list"}, 
					{"type":"list", "length":2}, 
					{"type":"string"}]}}},
	"struct-base":{
		"include":"struct-data_def_full_extended",
		"required":{
			"url-type":{
				"types":["integer"],
				"default":0},
			"data":{
				"types":["struct-tv_grab_data"]},
			"values":{
				"types":["struct-base_values"]}},
		"optional":{
			"text_replace":{
				"types":[
					{"type":"list"}, 
					{"type":"list", "length":2}, 
					{"type":"string"}]},
			"max days":{
				"types":["integer"],
				"default":14},
			"default-item-count":{
				"required_on":[{"key":[],"in":[12,13,14]}],
				"types":["integer"],
				"default":0}}},
	"struct-base_detailed":{
		"include":"struct-base",
		"required":{
			"values":{
				"types":["struct-base_detailed_values"]}}},
	"struct-detail":{
		"include":"struct-data_def_full_extended",
		"required":{
