					{"path":"parent"},
					{"path":"parent"},
					{"path":"parent"},
					{"attr":"data-episode-title"}]]},
		"values":{
			"channelid":{"varid": 1},
			"start-time":{"funcid":4, "data":[{"varid": 3},{"varid": 0}, 6]},
			"stop-time":{"funcid":4, "data":[{"varid": 3},{"varid": 2}, 6,{"varid": 0}]},
